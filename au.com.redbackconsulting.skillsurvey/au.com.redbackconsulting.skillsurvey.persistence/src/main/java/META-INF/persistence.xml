<?xml version="1.0" encoding="UTF-8"?>


<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
  
  	<persistence-unit name="au.com.redbackconsulting.skillsurvey.persistence" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Level</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Need</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Uoc</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Question</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Role</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Function</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Claim</class>
		<class>au.com.redbackconsulting.skillsurvey.persistence.model.Occupation</class>
		
			<exclude-unlisted-classes>true</exclude-unlisted-classes>
					<properties>
						<property name="eclipselink.ddl-generation" value="create-tables"/>
						<property name="eclipselink.ddl-generation.output-mode" value="database"/>
					</properties>
		</persistence-unit>
</persistence>
